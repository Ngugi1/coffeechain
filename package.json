{
    "name": "coffeechain-supply-chain",
    "version": "1.0.0",
    "description": "A supply network example in Hyperledger Fabric 1.4",
    "homepage": "https://github.com/Ngugi1/coffeechain",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "start": "node src/server/server.js",
        "test": "nyc mocha --recursive",
        "build": "npm update easychain && babel src/test.js -d dist"
    },
    "engineStrict": true,
    "author": "Iv√°n Alberquilla",
    "license": "Apache-2.0",
    "dependencies": {
        "arraybuffer-to-string": "^1.0.2",
        "express": "^4.17.1",
        "fabric-ca-client": "~1.4.0",
        "fabric-client": "^1.4.4",
        "fabric-network": "~1.4.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.6",
        "@babel/plugin-proposal-decorators": "^7.8.3",
        "babel-cli": "^6.26.0",
        "chai": "^4.2.0",
        "easychain": "file:easychain",
        "eslint": "^5.9.0",
        "mocha": "^5.2.0",
        "nyc": "^13.1.0",
        "sinon": "^7.1.1",
        "sinon-chai": "^3.3.0"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
